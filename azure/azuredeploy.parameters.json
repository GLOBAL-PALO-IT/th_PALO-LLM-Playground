{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "projectName": {
      "value": "palo-llm-playground"
    },
    "location": {
      "value": "eastus"
    },
    "containerImage": {
      "value": "YOUR_REGISTRY.azurecr.io/palo-llm-playground:latest"
    },
    "containerRegistry": {
      "value": "YOUR_REGISTRY.azurecr.io"
    },
    "containerRegistryUsername": {
      "value": "YOUR_REGISTRY_USERNAME"
    },
    "containerRegistryPassword": {
      "value": "YOUR_REGISTRY_PASSWORD"
    },
    "postgresAdminUsername": {
      "value": "pgadmin"
    },
    "postgresAdminPassword": {
      "value": "YOUR_POSTGRES_PASSWORD"
    },
    "postgresDatabaseName": {
      "value": "playground"
    },
    "openaiApiKey": {
      "value": "YOUR_OPENAI_API_KEY"
    },
    "livekitApiKey": {
      "value": ""
    },
    "livekitApiSecret": {
      "value": ""
    },
    "livekitUrl": {
      "value": ""
    },
    "tavilyApiKey": {
      "value": ""
    },
    "serperApiKey": {
      "value": ""
    },
    "minReplicas": {
      "value": 1
    },
    "maxReplicas": {
      "value": 3
    }
  }
}
